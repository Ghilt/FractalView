<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="91"
            android:viewportHeight="91">
            <path
                android:name="spiral"
                android:pathData="M 45.864 45.867 L 58.856 45.867 L 58.856 58.893 L 32.867 58.893 L 32.867 32.859 L 73.293 32.859 L 73.293 73.268 L 18.445 73.268 L 18.445 18.421 L 45.867 18.421"
                android:strokeColor="#fff"
                android:strokeWidth="10"
                android:strokeLineCap="square"/>
            <group android:name="group">
                <path
                    android:name="triangle_top"
                    android:pathData="M 38.487 28.433 L 53.247 28.433 L 53.247 43.193 L 38.487 43.193 Z"
                    android:fillColor="#fff"
                    android:fillAlpha="0"/>
                <path
                    android:name="triangle_mid"
                    android:pathData="M 11.312 25.811 L 80.195 25.811 L 80.195 51.917 L 11.312 51.917 Z"
                    android:fillColor="#fff"
                    android:fillAlpha="0"/>
                <path
