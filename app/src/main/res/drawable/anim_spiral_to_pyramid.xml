            <set>
                <objectAnimator
                    android:propertyName="strokeWidth"
                    android:duration="190"
                    android:valueFrom="10"
                    android:valueTo="15"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="202"
                    android:duration="40"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="triangle_top">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="pathData"
                    android:startOffset="190"
                    android:duration="310"
                    android:valueFrom="M 11.445 11.421 L 52.869 11.421 L 52.869 28.357 L 11.445 28.357 Z"
                    android:valueTo="M 38.487 28.433 L 53.247 28.433 L 53.247 43.193 L 38.487 43.193 Z"
                    android:valueType="pathType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillAlpha"
                    android:startOffset="190"
                    android:duration="10"
